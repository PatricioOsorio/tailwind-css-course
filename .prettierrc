{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-organize-attributes"
  ],
  "organizeAttributes": {
    "order": [
      "id",
      "class",
      "name",
      "data-*",
      "aria-*",
      "type",
      "href",
      "src",
      "for",
      "alt",
      "$DEFAULT"
    ]
  }
}
